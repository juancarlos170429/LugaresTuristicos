uscar\"\r\n                    className=\"textField\"\r\n                    name=\"busqueda\"\r\n                    value={this.state.busqueda}\r\n                    onChange={this.onChange}\r\n                    />\r\n                    <button type=\"button\" className=\"btnBuscar btn-primary\" /*onClick={onClear}*/>\r\n                    {\" \"}\r\n                    <BiIcons.BiSearchAlt/>\r\n                    </button>\r\n                </div>\r\n            <hr/>\r\n            <DataTable \r\n                columns={this.state.columnas}\r\n                data={this.state.lugares}\r\n                title=\"Lugares\"\r\n                pagination\r\n                paginationComponentOptions={paginacionOpciones}\r\n                fixedHeader\r\n                fixedHeaderScrollHeight=\"600px\"\r\n                width=\"600px\"\r\n                noDataComponent={<span>No se encontró ningún elemento</span>}\r\n            />\r\n            <hr/>\r\n        </div>\r\n        </div>\r\n    </div>\r\n  );\r\n}\r\n}\r\nexport default Buscar;","D:\\lugaresturisticos\\src\\Lugares\\Cusco.jsx",["41"],{"ruleId":"42","replacedBy":"43"},{"ruleId":"44","replacedBy":"45"},{"ruleId":"46","severity":1,"message":"47","line":3,"column":37,"nodeType":"48","messageId":"49","endLine":3,"endColumn":41},{"ruleId":"46","severity":1,"message":"47","line":1,"column":10,"nodeType":"48","messageId":"49","endLine":1,"endColumn":14},{"ruleId":"50","severity":1,"message":"51","line":99,"column":11,"nodeType":"52","endLine":99,"endColumn":138},{"ruleId":"53","severity":1,"message":"54","line":64,"column":40,"nodeType":"55","messageId":"56","endLine":64,"endColumn":42},{"ruleId":"53","severity":1,"message":"57","line":76,"column":30,"nodeType":"55","messageId":"58","endLine":76,"endColumn":32},{"ruleId":"59","severity":1,"message":"60","line":15,"column":29,"nodeType":"52","endLine":15,"endColumn":67},"no-native-reassign",["61"],"no-negated-in-lhs",["62"],"no-unused-vars","'Link